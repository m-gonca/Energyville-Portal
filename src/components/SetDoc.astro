---
import Heading from "../components/Heading.astro";
import Item from "./Item.astro";
export interface Props {
  title: string;
  img: string;
}
const { title, img } = Astro.props;
---

<div class="flex flex-col w-full pt-24 pl-40">
  <div class="flex flex-col h-full w-4/5 gap-14">
    <Heading title={title} />
    <div class="m-auto flex flex-wrap flex-row gap-y-10">
      <Item img={img} />
      <Item img={img} />
      <Item img={img} />
      <Item img={img} />
      <Item img={img} />
      <Item img={img} />
      <Item img={img} />
      <Item img={img} />
      <Item img={img} />
      <Item img={img} />
    </div>
  </div>
</div>
